{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          ">0.25% in JP",
          "not ie <= 10",
          "not op_mini all"
        ],
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
